    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"VT", " ", "=", 
      RowBox[{"NIntegrate", "[", 
       RowBox[{
        RowBox[{"q1", " ", "q2", " ", "q3", " ", 
         RowBox[{"Boole", "[", 
          RowBox[{"q1", "<=", 
           RowBox[{"q2", "+", "q3"}]}], "]"}], 
         RowBox[{"Boole", "[", 
          RowBox[{"q2", "<=", 
