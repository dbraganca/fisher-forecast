          RowBox[{"Boole", "[", 
           RowBox[{"q2", "<=", 
            RowBox[{"q1", "+", "q3"}]}], "]"}], 
          RowBox[{"Boole", "[", 
           RowBox[{"q3", "<=", 
            RowBox[{"q1", "+", "q2"}]}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"q1", ",", 
           RowBox[{"k1", "-", 
            FractionBox["\[CapitalDelta]k", "2"]}], ",", 
           RowBox[{"k1", "+", 
            FractionBox["\[CapitalDelta]k", "2"]}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"q2", ",", 
           RowBox[{"k2", "-", 
