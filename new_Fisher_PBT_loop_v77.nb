        RowBox[{
         RowBox[{"q1", " ", "q2", " ", "q3", " ", 
          RowBox[{"Max", "[", 
           RowBox[{"{", 
            RowBox[{"q1", ",", "q2", ",", "q3"}], "}"}], "]"}], " ", 
          RowBox[{"Boole", "[", 
           RowBox[{"q1", "<=", 
